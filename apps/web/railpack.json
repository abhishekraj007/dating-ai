{
  "$schema": "https://schema.railpack.com",
  "steps": {
    "install": {
      "commands": ["corepack enable", "pnpm install --no-frozen-lockfile"]
    },
    "build": {
      "commands": ["...", "pnpm --filter web build"]
    }
