{
  "$schema": "https://schema.railpack.com",
  "steps": {
    "install": {
      "commands": ["npm install -g pnpm", "pnpm install --no-frozen-lockfile"]
    },
    "build": {
      "commands": ["...", "pnpm --filter admin build"]
    }
  },
  "deploy": {
    "startCommand": "pnpm --filter admin start"
  }
}
